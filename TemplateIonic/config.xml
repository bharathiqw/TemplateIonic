<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.ost01638294" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Mansha</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
      Ionic Framework Team
    </author>
  <content src="index.html" />
  <allow-navigation href="http://example.com/*" />
  <!-- Wildcards are allowed for the protocol, as a prefix
         to the host, or as a suffix to the path -->
  <allow-havigation href="*://*.example.com/*" />
  <!-- A wildcard can be used to whitelist the entire network,
         over HTTP and HTTPS.
         *NOT RECOMMENDED* -->
  <allow-navigation href="*" />
  <allow-navigation href="http://*/*" />
  <allow-navigation href="https://*/*" />
  <allow-navigation href="data:*" />
  <access origin="*" />
  <allow-intent href="*" />
  <allow-navigation href="*" />
  <!-- The above is equivalent to these three declarations -->
  <preference name="webviewbounce" value="false" />
  <preference name="UIWebViewBounce" value="false" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="android-minSdkVersion" value="16" />
  <preference name="android-minSdkVersion" value="18" />
  <preference name="android-targetSdkVersion" value="21" />
  <preference name="BackupWebStorage" value="none" />
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true" />
  </feature>
  <plugin name="com.ionic.keyboard" version="1.0.4" />
  <plugin name="cordova-plugin-console" version="1.0.1" />
  <plugin name="cordova-plugin-device" version="1.0.1" />
  <plugin name="cordova-plugin-splashscreen" version="2.1.0" />
  <plugin name="cordova-sqlite-storage" version="2.0.4" src="https://github.com/brodysoft/Cordova-SQLitePlugin.git" />
  <plugin name="cordova-plugin-whitelist" version="1.3.2" />
  <plugin name="phonegap-plugin-push" version="1.8.4">
    <variable name="SENDER_ID" value="249225983946 " />
  </plugin>
  <!--<gap:plugin name="com.adobe.plugins.pushplugin" />-->
</widget>